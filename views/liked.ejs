<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="../public/css/style.css" />
    <title>Liked</title>
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;700&display=swap");
    </style>
  </head>

  <body>
    <header><%- include('./header'); %></header>
    <h1 class="CenterText">Liked profiles</h1>
    <section class="LikedListContainer">
      <table class="TableSpacing">
        <%likesList.forEach(likes => {%>
        <tr>
          <td><%= likes.name %></td>
          <td><%= likes.age %></td>
          <td>
            <a
              ><img class="LikedListIcon" src="/public/images/SkipIcon.png"
            /></a>
          </td>
          <td>
            <a
              ><img class="LikedListIcon" src="/public/images/ChatIcon.png"
            /></a>
          </td>
        </tr>
        <%})%>
      </table>
    </section>

    <footer><%- include('./footer'); %></footer>
  </body>
</html>
