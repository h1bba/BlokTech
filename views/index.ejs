<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="/css/style.css" />
    <style>
      @import url("https://fonts.googleapis.com/css2?family=Poppins:wght@400;500;700&display=swap");
    </style>
    <title>Home</title>
  </head>

  <body>
    <header><%- include('./header') %></header>

    <table class="TableSpacing">
      <% likesList.forEach(likes => { %>
      <tr>
        <td><%= likes.name %></td>
        <td><%= likes.age %></td>
        <td>
          <a href="/unlike/<%= likes._id %>">
            <img class="LikedListIcon" src="/public/images/SkipIcon.png" />
          </a>
        </td>
        <td>
          <a><img class="LikedListIcon" src="/public/images/ChatIcon.png" /></a>
        </td>
      </tr>
      <% }) %>
    </table>



      <section class="swipeButtons">
        <a><img src="/public/images/SkipIcon.png" /></a>
        <a><img src="/public/images/HeartIcon.png" /></a>
      </section>
    </section>

    <footer><%- include('./footer') %></footer>
  </body>
</html>
